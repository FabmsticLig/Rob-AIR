<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find roswifibot)/configuration/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find roswifibot)/configuration/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find roswifibot)/configuration/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find roswifibot)/configuration/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find roswifibot)/configuration/base_local_planner_params.yaml" command="load" />
	</node>
</launch>